          <eTypeArguments eTypeParameter="#//IPropertyHolder/getProperty/T"/>
        </eGenericType>
      </eParameters>
    </eOperations>
    <eOperations name="hasProperty" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
