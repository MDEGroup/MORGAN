  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="InputType">
    <eLiterals name="TEXT"/>
    <eLiterals name="NUMBER" value="1"/>
    <eLiterals name="DATE" value="6"/>
    <eLiterals name="EMAIL" value="7"/>
    <eLiterals name="RANGE" value="9"/>
